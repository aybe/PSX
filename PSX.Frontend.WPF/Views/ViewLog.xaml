<Window x:Class="PSX.Frontend.WPF.Views.ViewLog"
        x:ClassModifier="internal"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:modules="clr-namespace:PSX.Frontend.Core.Modules;assembly=PSX.Frontend.Core"
        mc:Ignorable="d"
        d:DataContext="{d:DesignInstance modules:ViewModelLog}"
        Title="PSX Log" Left="0" Top="0" WindowStartupLocation="Manual">
    <Grid>
        <ListBox ItemsSource="{Binding Entries}"
                 VirtualizingStackPanel.VirtualizationMode="Recycling"
                 d:ItemsSource="{d:SampleData}" />
    </Grid>
</Window>